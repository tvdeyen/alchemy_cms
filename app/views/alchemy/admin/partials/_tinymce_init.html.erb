<script>
  // Setting TinyMCE path.
  var tinyMCEPreInit = {
    base: '<%= asset_url(assets_prefix + '/tinymce') %>',
    suffix: '.min'
  };

  // Holds the default Alchemy TinyMCE configuration
  Alchemy.Tinymce = {
    defaults: {
      plugins: '<%= Alchemy::Tinymce.plugins.join(',') %>',
      <% Alchemy::Tinymce.init.each do |k, v| %>
      <%= k %>: <%== v.to_json %>,
      <% end %>
    }
  };
</script>
